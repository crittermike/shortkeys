    "gulp-sequence": "0.x.x",
    "gulp-sourcemaps": "^2.x.x",
    "gulp-tag-version": "1.x.x",
    "gulp-util": "3.x.x",
    "gulp-zip": "^4.x.x",
    "require-dir": "0.x.x",
    "standard": "^10.0.2",
    "vinyl-named": "1.x.x",
    "webpack": "3.x.x",
    "webpack-stream": "3.x.x",
    "yargs": "^8.x.x"
